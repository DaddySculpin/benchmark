{
  "name": "@coreym/benchmark",
  "description": "Benchmark Design System",
  "author": "Corey McGeachie",
  "license": "MIT",
  "version": "0.1.0",
  "private": true,
  "main": "dist/components/index.js",
  "prepublish": "npm run release",
  "files": [
    "dist/components/index.js"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src; exit 0",
    "lint:fix": "eslint --fix ./src; exit 0",
    "styleguidist": "styleguidist server",
    "storybook": "start-storybook -p 5000",
    "docs": "gatsby develop",
    "playroom": "playroom start",
    "stats": "webpack --profile --json > webpack-stats.json",
    "prebuild": "rimraf dist && rimraf public",
    "build": "npm run build:main && npm run build:docs && npm run build:storybook && npm run build:playroom",
    "build:main": "webpack -p ./src/index.js",
    "build:docs": "gatsby build",
    "build:storybook": "build-storybook -o dist/storybook",
    "build:playroom": "playroom build",
    "build:watch": "webpack --watch -p ./src/index.js",
    "release": "npm run lint && npm run test && npm run build"
  },
  "dependencies": {
    "@reach/auto-id": "^0.6.1",
    "@styled-system/css": "^5.0.23",
    "@styled-system/should-forward-prop": "^5.1.4",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react-icons": "^3.8.0",
    "react-roving-tabindex": "^0.7.1",
    "styled-system": "^5.1.2"
  },
  "peerDependencies": {
    "@emotion/core": "10.x",
    "@emotion/styled": "10.x",
    "emotion-theming": "10.x",
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@mdx-js/mdx": "^1.4.0",
    "@mdx-js/react": "^1.4.0",
    "@storybook/addon-a11y": "^5.2.6",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-storysource": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.6",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-gatsby": "^0.2.20",
    "cross-env": "^6.0.3",
    "css-loader": "^3.1.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "fs-extra": "^8.1.0",
    "gatsby": "^2.14.0",
    "gatsby-plugin-catch-links": "^2.1.5",
    "gatsby-plugin-mdx": "^1.0.33",
    "gatsby-plugin-page-creator": "^2.1.16",
    "gatsby-source-filesystem": "^2.1.18",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-axe": "^3.2.0",
